{
  "name": "irmaosangare",
  "version": "1.0.0",
  "description": "\u0000",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && next dev && test:watch",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "start": "node index.js",
    "migration:up:local": "npx node-pg-migrate -m infra/migrations up --envPath=.env.development",
    "migration:down:local": "npx node-pg-migrate -m infra/migrations down --envPath=.env.development",
    "migration:create": "npx node-pg-migrate create -m infra/migrations"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "framer-motion": "^12.23.24",
    "next": "^13.1.6",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^7.9.5",
    "react-toggle-dark-mode": "^1.1.1",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "jest": "^29.6.2",
    "node-pg-migrate": "^8.0.3",
    "prettier": "^3.6.2"
  }
}
